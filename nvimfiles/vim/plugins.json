{
  "default": {},
  "plugins": {
    // Theme
    "chriskempson/base16-vim": {
      "set": {
        "base16colorspace": 256 
      }
    },
    // "godlygeek/csapprox": {},
    // "altercation/vim-colors-solarized": {
    // },

    // "Raimondi/delimitMate": {},
    // UI
    "bling/vim-airline": {
      "set": {
        "g:airline_powerline_fonts": 1,
        "g:airline#extensions#tabline#enabled": 1
      }
    },
    "airblade/vim-gitgutter": {},
    // "kana/vim-arpeggio": {},

    // "KabbAmine/vCoolor.vim": {},
    "iandoe/vim-osx-colorpicker": {
      "set": {
        "g:colorpicker_app": "iTerm.app"
      },
      "on": "Color"
    },

    // Tools
    "Shougo/unite.vim": {
      "map": [
        ["nnore",  "[unite]",    "<nop>"],
        ["n",      ",",          "[unite]"],
        ["nnore",  "[unite]@",   ":<C-u>Unite -buffer-name=register register<CR>"],
        ["nnore",  "[unite]a",   ":<C-u>Unite -buffer-name=ag grep:.:-SC2<cr>"],
        ["nnore",  "[unite]ag",  ":<C-u>Unite-buffer-name=ag_root vcs_grep/ag_root<cr>"],
        ["nnore",  "[unite]b",   ":<C-u>Unite -buffer-name=buffer buffer<cr>"],
        ["nnore",  "[unite]cf",  ":<C-u>UniteWithBufferDir -buffer-name=files file<cr>"],
        ["nnore",  "[unite]cg",  ":<C-u>UniteWithBufferDir -buffer-name=files git_tree<cr>"],
        ["nnore",  "[unite]cr",  ":<C-u>UniteWithBufferDir -buffer-name=files file_rec/async<cr>"],
        ["nnore",  "[unite]ff",  ":<C-u>Unite -buffer-name=files file<cr>"],
        ["nnore",  "[unite]fg",  ":<C-u>Unite -buffer-name=files git_tree<cr>"],
        ["nnore",  "[unite]fr",  ":<C-u>Unite -buffer-name=files file_rec/async<cr>"],
        ["nnore",  "[unite]gg",  ":<C-u>Unite -buffer-name=git_grep vcs_grep/git:-i<cr>"],
        ["nnore",  "[unite]o",   ":<C-u>Unite -buffer-name=outline outline<cr>"],
        ["nnore",  "[unite]r",   ":<C-u>Unite -buffer-name=mru file_mru<cr>"],
        ["nnore",  "[unite]sl",  ":<C-u>Unite -buffer-name=session session<cr>"],
        ["nnore",  "[unite]ss",  ":<C-u>Unite -buffer-name=session session/new<cr>"],
        ["nnore",  "[unite]w",   ":<C-u>UniteWithCursorWord -buffer-name=ag grep:.:-SC2<cr>"],
        ["nnore",  "[unite]y",   ":<C-u>Unite -buffer-name=yank -no-start-insert history/yank<cr>"]
      ]
    },
    "Shougo/neomru.vim": {
      // "on": "Unite"
    },
    "Shougo/unite-session": {
      // "on": "Unite"
    },
    "Shougo/vimproc.vim": {
      "do": "make"
    },
    "davidhalter/jedi-vim": {
      "set": {
        "g:jedi#auto_vim_configuration": 0,
        "g:jedi#auto_initialization": 0
      },
      "do": "pip install --upgrade jedi"
    },
    "rking/ag.vim": {},
    // "christoomey/vim-conflicted": {
    "norcalli/vim-conflicted/": {
      "on": "Conflicted"
    },
    "mattn/emmet-vim": {
      "for": "html"
    },
    "godlygeek/tabular": {
      "on": ["Tabularize"]
    },
    "SirVer/ultisnips": {
      "on": [],
      "set": {
        "g:UltiSnipsEditSplit": "vertical",
        "g:UltiSnipsExpandTrigger": "ý",
        "g:UltiSnipsJumpForwardTrigger": "ý",
        "g:UltiSnipsJumpBackwardTrigger": "û"
      }
    },
    "majutsushi/tagbar": {
      "map": [
        ["nnore", "<M-C-T>", ":TagbarToggle<cr>"]
      ]
      // "on": ["TagbarToggle", "Tagbar"]
    },
    "marijnh/tern_for_vim": {
      "for": "javascript",
      "do": "npm install"
    },
    "Peeja/vim-cdo": {
      "on": ["Ldo", "Cdo"]
    },
    "bogado/file-line": {},
    "rhysd/vim-clang-format": {
      "for": "cpp",
      "set": {
        "g:clang_format#code_style": "google",
        "g:clang_format#auto_format": 0,
        "g:clang_format#style_options": {
          "ColumnLimit": 100
        }
      }
    },
    "artnez/vim-wipeout": {
      "on": "Wipeout"
    },
    // "ehamberg/vim-cute-python": {},


    "Valloric/YouCompleteMe": {
      "postinstall": "./install.sh",
      "do": "git submodule update --init --recursive; ./install.sh",
      "set": {
        "g:ycm_global_ycm_extra_conf": "~/.vim/ycm_conf.py"
      }
    },

    // Tim Pope
    "tpope/vim-commentary": {},
    "tpope/vim-fugitive": {},
    "tpope/vim-unimpaired": {},
    "tpope/vim-sensible": {},
    "tpope/vim-surround": {},
    "tpope/vim-repeat": {},
    "tpope/vim-abolish": {},

    // Navigation
    "justinmk/vim-sneak": {},
    // "gorkunov/smartgf.vim": {
    //   "set": {
    //     "g:smartgf_extensions": [".vim", ".ls", ".coffee", ".js"]
    //   }
    // },
    // "kana/vim-gf-user": { },
    // "bkad/CamelCaseMotion": {},

    // Textobjects
    "kana/vim-textobj-user": {},
    "kana/vim-textobj-indent": {},
    // "kana/vim-textobj-line": {},
    // "kana/vim-textobj-entire": {},
    // "kana/vim-textobj-lastpat": {},
    "norcalli/textobj-gitgutter": {},
    "gaving/vim-textobj-argument": {},
    // "Julian/vim-textobj-brace": {},
    "coderifous/textobj-word-column.vim": {},
    "beloglazov/vim-textobj-quotes": {},
    // "glts/vim-textobj-comment": {},

    // Operator
    "kana/vim-operator-user": {},
    "kana/vim-operator-replace": {},

    // File types
    "mustache/vim-mustache-handlebars": {
      "set": {
        "g:mustache_abbreviations": 1,
        "g:mustache_operators": 0
      }
    },
    "cakebaker/scss-syntax.vim": {
      "for": "scss"
    },
    "cespare/vim-go-templates": {
      "for": "go"
    },
    // "elzr/vim-json": {
    //   "set": {
    //     "g:vim_json_syntax_conceal": 0
    //   }
    // },
    "chrisbra/Colorizer": {},
    "fatih/vim-go": {
      "for": "go",
      "set": {
        "g:go_fmt_command": "goimports"
      }
    },
    "gkz/vim-ls": {
      "for": "ls"
    },
    "honza/vim-snippets": {},
    "kchmck/vim-coffee-script": {
      "for": "coffee"
    },
    "pangloss/vim-javascript": {
      "for": "javascript"
    },
    "othree/javascript-libraries-syntax.vim": {
      "for": "javascript"
    },
    // "awetzel/vim-elixir": {
    //   "dir": "~/.vim/cobalt/awetzel.vim-elixir/",
    "norcalli/vim-elixir": {
      "branch": "nvim-rplugin",
      // "for": "elixir",
      "set": {
        "g:elixir_load_remote": 1
      }
    },
    // "elixir-lang/vim-elixir": {},
    "zah/nimrod.vim": {
      "for": "nim"
    }
  }
}
